<h1>Articles</h1>

<ul>
  <% @articles.each do |article| %>
      <li>
        <%= link_to article.title, article %>
      </li>
  <% end %>
</ul>

<div class="newart text-center">
  <div class="container-fluid py-5">
    <button class=" homebutton btn btn-outline-light" type="button"><%= link_to "New Article", new_article_path %></button>
  </div>
</div>
